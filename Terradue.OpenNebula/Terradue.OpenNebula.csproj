<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageVersion>2.0.0</PackageVersion>
    <Authors>Enguerran Boissier</Authors>
    <Copyright>2021</Copyright>
    <PackageLicenseUrl>https://github.com/Terradue/DotNet4One/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Terradue/DotNet4One</PackageProjectUrl>
    <Title>Terradue.OpenNebula</Title>
    <Description>DotNet4One is a library targeting .NET 4.0 and above providing an easy way to perform requests on any XML-RPC method exposed by an OpenNebula server.</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Kveer.XmlRPC" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Kveer.XmlRPC" Version="1.2.0" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
</Project>
